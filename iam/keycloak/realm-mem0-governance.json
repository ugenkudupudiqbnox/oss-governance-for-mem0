{
  "realm": "mem0-governance",
  "enabled": true,
  "roles": {
    "realm": [
      { "name": "admin" },
      { "name": "agent-reader" },
      { "name": "agent-writer" },
      { "name": "auditor" }
    ]
  },
  "clients": [
    {
      "clientId": "mem0-governance-api",
      "protocol": "openid-connect",
      "publicClient": false,
      "serviceAccountsEnabled": true,
      "directAccessGrantsEnabled": true
    },
    {
      "clientId": "grafana",
      "protocol": "openid-connect",
      "publicClient": false,
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "secret": "grafana-client-secret",
      "redirectUris": [
        "https://grafana.audit.qbnox.com/*",
        "http://localhost:9003/*"
      ],
      "webOrigins": [
        "https://grafana.audit.qbnox.com",
        "http://localhost:9003"
      ]
    }
  ]
}
